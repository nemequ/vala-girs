        <parameters>
          <parameter name="appbits"
                     direction="out"
                     caller-allocates="0"
                     transfer-ownership="full">
            <doc xml:whitespace="preserve">Application specific bits</doc>
            <type name="guint8" c:type="guint8*"/>
          </parameter>

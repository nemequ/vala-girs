            <type name="Gio.File" c:type="GFile*"/>
          </parameter>
          <parameter name="level" transfer-ownership="none">
            <doc xml:space="preserve">a #GgitConfigLevel.</doc>
            <type name="ConfigLevel" c:type="GgitConfigLevel"/>

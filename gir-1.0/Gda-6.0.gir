        <return-value transfer-ownership="full">
          <doc xml:space="preserve">@worker</doc>
          <type name="Worker" c:type="GdaWorker*"/>
        </return-value>
        <parameters>
          <instance-parameter name="worker" transfer-ownership="none">
            <doc xml:space="preserve">a #GdaWorker</doc>
            <type name="Worker" c:type="GdaWorker*"/>
          </instance-parameter>

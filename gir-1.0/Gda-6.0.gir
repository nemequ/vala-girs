          <parameter name="out_result"
                     transfer-ownership="none"
                     nullable="1"
                     allow-none="1">
            <doc xml:space="preserve">a place to store the value returned by the execution of the requested function within the worker thread, or %NULL</doc>
            <type name="gpointer" c:type="gpointer*"/>
          </parameter>

      <member name="country"
              value="2"
              c:identifier="GWEATHER_LOCATION_COUNTRY"
              glib:nick="country">
        <doc xml:space="preserve">A location representing a "country" (or
other geographic unit that has an ISO-3166 country code)</doc>
      </member>
      <member name="adm1"
              value="3"
              c:identifier="GWEATHER_LOCATION_ADM1"
              glib:nick="adm1">
        <doc xml:space="preserve">A location representing a "first-level
administrative division"; ie, a state, province, or similar
division.</doc>
      </member>
      <member name="adm2"
              value="4"
              c:identifier="GWEATHER_LOCATION_ADM2"
              glib:nick="adm2">
        <doc xml:space="preserve">A location representing a subdivision of a
%GWEATHER_LOCATION_ADM1 location, or a direct subdivision of
a country that is not represented in a #GWeatherLocationEntry.</doc>
      </member>
      <member name="city"
              value="5"
              c:identifier="GWEATHER_LOCATION_CITY"
              glib:nick="city">
        <doc xml:space="preserve">A location representing a city</doc>
      </member>
      <member name="weather_station"
              value="6"
              c:identifier="GWEATHER_LOCATION_WEATHER_STATION"

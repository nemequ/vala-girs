          <parameter name="y"
                     direction="out"
                     caller-allocates="0"
                     transfer-ownership="full">
            <doc xml:space="preserve">returns the y coordinate of the origin of the item's coordinate space.</doc>
            <type name="gdouble" c:type="gdouble*"/>

          <parameter name="part" transfer-ownership="none">
            <doc xml:whitespace="preserve">a URI part</doc>
            <type name="utf8" c:type="const char*"/>
          </parameter>
          <parameter name="unescape_extra" transfer-ownership="none">
            <doc xml:whitespace="preserve">reserved characters to unescape (or %NULL)</doc>
            <type name="utf8" c:type="const char*"/>
          </parameter>
        </parameters>
      </function>

        <return-value transfer-ownership="none">
          <type name="none" c:type="void"/>
        </return-value>
        <parameters>
          <instance-parameter name="pager" transfer-ownership="none">
            <doc xml:space="preserve">a #WnckPager.</doc>
            <type name="Pager" c:type="WnckPager*"/>
          </instance-parameter>

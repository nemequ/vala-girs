        <doc xml:space="preserve">Gets whether @window is maximized horizontally. Horizontal maximization
state may change anytime a #WnckWindow::state-changed signal gets emitted.</doc>
        <return-value transfer-ownership="none">
          <doc xml:space="preserve">%TRUE if @window is maximized horizontally, %FALSE otherwise.</doc>
          <type name="gboolean" c:type="gboolean"/>
        </return-value>
        <parameters>
          <instance-parameter name="window" transfer-ownership="none">
            <doc xml:space="preserve">a #WnckWindow.</doc>
            <type name="Window" c:type="WnckWindow*"/>

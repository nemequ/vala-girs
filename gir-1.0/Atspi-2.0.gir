      <type name="gint" c:type="gint"/>
    </constant>
    <constant name="STATETYPE_COUNT" value="42" c:type="ATSPI_STATETYPE_COUNT">
      <doc xml:space="preserve">One higher than the highest valid value of #AtspiStateType.</doc>

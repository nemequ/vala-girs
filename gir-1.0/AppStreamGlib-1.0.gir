          <type name="AppScope" c:type="AsAppScope"/>
        </parameter>
        <parameter name="bundle_kind" transfer-ownership="none">
          <doc xml:space="preserve">System, e.g. 'package' or 'flatpak'</doc>
          <type name="BundleKind" c:type="AsBundleKind"/>

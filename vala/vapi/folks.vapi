	[CCode (cheader_filename = "folks/folks.h")]
	public enum PresenceType {
		/**
		 * never set
		 */
		UNSET,
		/**
		 * offline
		 */
		OFFLINE,
		/**
		 * available
		 */
		AVAILABLE,
		/**
		 * away from keyboard
		 */
		AWAY,
		/**
		 * away from keyboard for an extended period of time
		 */
		EXTENDED_AWAY,
		/**
		 * also known as "invisible" or "appear offline"
		 */
		HIDDEN,
		/**
		 * at keyboard, but too busy to chat
		 */
		BUSY,
		/**
